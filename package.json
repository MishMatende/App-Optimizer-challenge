{
  "name": "App-Optimizer-challenge",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "clear&&next dev --turbo | tee out.txt",
    "build": "next build",
    "start": "next start",
    "lint": "npx @biomejs/biome lint . --apply",
    "generate": "drizzle-kit generate:pg",
    "migrate": "drizzle-kit push:pg ",
    "dev_migrate": "set NODE_ENV=development&& drizzle-kit push:pg ",
    "format": "npx @biomejs/biome format . --apply",
    "unsafe-format": "npx @biomejs/biome check --apply-unsafe .",
    "introspect": "drizzle-kit introspect:pg",
    "setupdb": "npm run introspect&&npm run generate&&npm run migrate",
    "refresh": "rm -r .next&&npx next telemetry disable&&npm run dev",
    "update-types": "npx supabase gen types typescript --project-id \"ghtrmopaptjeinhmytip\" --schema public > ./src/server/db/supabase.types.ts --schema auth > ./src/server/db/supabase.auth.types.ts",
    "echo": "node ./scriptease.js"
  },
